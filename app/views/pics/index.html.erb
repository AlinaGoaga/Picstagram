<div class="image_list">
  <% @pics.each do |pic| %>
  <div class="image_list_individual">
  <%= link_to image_tag(pic.picture.url, class: "thumbnail"), pic %>
      <%= link_to 'Edit', edit_pic_path(pic) %>
      <%= link_to 'Destroy', pic, method: :delete, data: { confirm: 'Are you sure?' } %>
    </div>
  <% end %>
</div>
