<div class="pic-list">
  <% @pics.each do |pic| %>
  <div class="pic-card">
  <%= link_to image_tag(pic.picture.url, class: "pic-card__image"), pic %>
      <%= link_to 'Edit', edit_pic_path(pic) %>
      <%= link_to 'Destroy', pic, method: :delete, data: { confirm: 'Are you sure?' } %>
    </div>
  <% end %>
</div>
